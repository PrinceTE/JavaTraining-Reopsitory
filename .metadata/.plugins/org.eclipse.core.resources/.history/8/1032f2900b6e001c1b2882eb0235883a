package com.te.javabasics.banl;

public class AccountTest {

	public static void main(String[] args) {
		Account account=new Account(20000);
		try {
			account.withdraw(10000);
			account.withdraw(10000);
			account.withdraw(10000);
		} catch (Exception e) {

			System.out.println(e.getMessage());
		}
	}
}
